<!-- Head -->
<%- include('../template/head') %>

    <body>
        <div id="app">
            <!-- Sidebar -->
            <%- include('../template/sidebar') %>
                <div id="main">

                    <!-- Header -->
                    <%- include('../template/header') %>

                        <div class="page-heading">
                            <div class="page-title">
                                <div class="row">
                                    <div class="col-12 col-md-6 order-md-1 order-last">
                                        <h3>Transaksi Sawerin</h3>
                                    </div>
                                    <div class="col-12 col-md-6 order-md-2 order-first">
                                        <nav aria-label="breadcrumb" class="breadcrumb-header float-start float-lg-end">
                                            <ol class="breadcrumb">
                                                <li class="breadcrumb-item"><a
                                                        href="http://127.0.0.1:3030/admin/dashboard">Dashboard</a></li>
                                                <li class="breadcrumb-item active" aria-current="page">Transaksi Sawerin
                                                </li>
                                            </ol>
                                        </nav>
                                    </div>
                                </div>
                            </div>
                        </div>


                        <!-- Basic Tables start -->
                        <section class="section">
                            <div class="card shadow">
                                <div class="card-body">
                                    <table class="table" id="table1">
                                        <thead>
                                            <tr>
                                                <th>ID Transaksi</th>
                                                <th>Reff PG ID</th>
                                                <th>Pengirim</th>
                                                <th>Metode Pembayaran</th>
                                                <th>Waktu Transaksi</th>
                                                <th>Status Transaksi</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <% trx_nyawer.forEach((el)=> {%>
                                                <tr>
                                                    <td>
                                                        <a href="/admin/transaksi_nyawer/<%= el.id_transaksi%>">
                                                            <%= el.id_transaksi%>
                                                        </a>
                                                    </td>
                                                    <td>
                                                        <%= el.reff_pg%>
                                                    </td>
                                                    <td>
                                                        <%= el.nama_pengirim%>
                                                    </td>
                                                    <td>
                                                        <%= el.nama_mp%>
                                                    </td>
                                                    <td>
                                                        <%= el.waktu_transaksi%>
                                                    </td>
                                                    <td>
                                                        <%= el.status_transaksi%>
                                                    </td>
                                                </tr>
                                                <% }) %>
                                        </tbody>
                                    </table>
                                </div>
                            </div>

                        </section>
                        <!-- Basic Tables end -->

                        <!-- Footer -->
                        <%- include('../template/footer') %>
                </div>
        </div>

        <!-- JS -->
        <%- include('../template/js') %>

            <script src="https://cdn.jsdelivr.net/gh/zuramai/mazer@docs/demo/assets/static/js/pages/datatables.js"></script>
    </body>

    </html>